<h1>Feed</h1>

<div class="container">

  <div class="row">
     <div class="col s12 m7">
       <% @following.each do |name| %>
        <%post = Blog.new(current_user).following_photos(name) %>
         <div class="card blue-grey darken-1">
           <% post[:photos].each do |url| %>
             <div class="card-image">
               <%= image_tag("#{url}") %>
             </div>
            <% end %>
             <div class="card-content">
               <p><%= name %></p>
               <p><%= post[:summary] %></p>
            </div>
         </div> <br><br>
        <% end %>
     </div>
  </div>
</div>
